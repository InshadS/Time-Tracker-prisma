//   name     String
//   email    String @unique
//   password String
// }

model passportUser {
  id     String  @id
  name   String
  avatar String?
  tasks  Tasks[]
}

model Tasks {
  id            Int          @id @default(autoincrement())
  task          String?
  start_time    String?
  end_time      String?
  task_duration String?
  user          passportUser @relation(fields: [user_id], references: [id], onDelete: Cascade)
  user_id       String
}
